<div class="panel panel-default">
  <div class="panel-heading">
    message
  </div>
  <div class="panel-body">
    <% @messages.each do |message| %>
    <span class="label label-info"><%= message.name %></span>
    <%= message.body %>
    <br />
    <% end %>
  </div>
</div>

<%= form_for(@message) do |f| %>

name:

<%= f.text_field :name %>

what:

<%= f.text_field :body %>

<%= f.submit class: "btn btn-sm btn-primary"%>
<% end %>
